<div class="sm:mx-2 my-2">
    <table class="text-left w-full rounded-md">
        <thead>
        <tr>
            <th class="py-2 px-4 font-bold uppercase text-sm text-gray-600">
                AWS Account Name
            </th>
            <th class="py-2 px-4 font-bold uppercase text-sm text-gray-600">
                Status
            </th>
        </tr>
        </thead>
        <% data.serviceItems.forEach((item) => { %>
            <tr class="text-gray-800">
                <td class="py-2 px-4">
                    <%= item.organizationName %><br />
                    <small class="text-sm text-gray-500"><%= item.organizationEmail %></small>
                </td>
                <td class="py-2 px-4">
                    <%= item.organizationStatus %>
                </td>
            </tr>
        <% }); %>
    </table>
</div>
