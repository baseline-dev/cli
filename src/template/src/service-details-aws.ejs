<div class="sm:mx-2 my-2">
    <table class="text-left w-full rounded-md">
        <thead>
        <tr>
            <th class="py-2 px-4 font-bold uppercase text-sm text-gray-600">
                Account Name
            </th>
            <th class="py-2 px-4 font-bold uppercase text-sm text-gray-600">
                Account Status
            </th>
        </tr>
        </thead>
        <% data.serviceItems.forEach((item) => { %>
            <tr class="text-gray-800">
                <td class="py-2 px-4">
                    <%= item.accountName %><br />
                    <small class="text-sm text-gray-500"><%= item.accountEmail %></small>
                </td>
                <td class="py-2 px-4">
                    <%= item.accountStatus %>
                </td>
            </tr>
        <% }); %>
    </table>
</div>
