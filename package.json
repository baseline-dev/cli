{
  "name": "baseline-cli",
  "version": "0.0.8",
  "description": "Baseline your SaaS accounts.",
  "main": "src/index.js",
  "bin": {
    "baseline": "src/index.js"
  },
  "scripts": {
    "test": "npm run lint && npm run coverage",
    "test:unit": "tape --require esm test/*.js test/**/*.js | tap-spec",
    "lint": "",
    "coverage": "nyc --require esm --reporter=lcov --reporter=text-summary npm run test:unit",
    "build": "rollup src/index.js --file build/cli.js --format cjs & pkg dist/cli.js --out-path dist"
  },
  "author": "Nikolai Onken",
  "license": "Apache 2.0",
  "dependencies": {
    "chalk": "^3.0.0",
    "esm": "^3.2.25",
    "open": "^7.0.2",
    "ora": "^4.0.3",
    "portfinder": "^1.0.25",
    "prompts": "^2.3.1"
  },
  "devDependencies": {
    "node-env-file": "^0.1.8",
    "nyc": "^15.0.0",
    "rollup": "^1.32.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.2"
  }
}
